{
  "name": "@preconstruct/cli",
  "version": "2.8.9",
  "description": "Dev and build your code painlessly in monorepos",
  "exports": {
    "./cli": {
      "types": {
        "import": "./cli/dist/preconstruct-cli-cli.cjs.mjs",
        "default": "./cli/dist/preconstruct-cli-cli.cjs.js"
      },
      "module": "./cli/dist/preconstruct-cli-cli.esm.js",
      "import": "./cli/dist/preconstruct-cli-cli.cjs.mjs",
      "default": "./cli/dist/preconstruct-cli-cli.cjs.js"
    },
    "./worker": {
      "types": {
        "import": "./worker/dist/preconstruct-cli-worker.cjs.mjs",
        "default": "./worker/dist/preconstruct-cli-worker.cjs.js"
      },
      "module": "./worker/dist/preconstruct-cli-worker.esm.js",
      "import": "./worker/dist/preconstruct-cli-worker.cjs.mjs",
      "default": "./worker/dist/preconstruct-cli-worker.cjs.js"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "bin.js",
    "cli",
    "worker",
    "!**/*.d.ts",
    "dist"
  ],
  "bin": {
    "preconstruct": "./bin.js"
  },
  "repository": "https://github.com/preconstruct/preconstruct/tree/main/packages/cli",
  "license": "MIT",
  "dependencies": {
    "@babel/code-frame": "^7.5.5",
    "@babel/core": "^7.7.7",
    "@babel/helper-module-imports": "^7.10.4",
    "@babel/runtime": "^7.7.7",
    "@preconstruct/hook": "^0.4.0",
    "@rollup/plugin-alias": "^3.1.1",
    "@rollup/plugin-commonjs": "^15.0.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^11.2.1",
    "@rollup/plugin-replace": "^2.4.1",
    "builtin-modules": "^3.1.0",
    "chalk": "^4.1.0",
    "ci-info": "^3.8.0",
    "dataloader": "^2.0.0",
    "detect-indent": "^6.0.0",
    "enquirer": "^2.3.6",
    "estree-walker": "^2.0.1",
    "fast-deep-equal": "^2.0.1",
    "fast-glob": "^3.2.4",
    "fs-extra": "^9.0.1",
    "is-reference": "^1.2.1",
    "jest-worker": "^26.3.0",
    "magic-string": "^0.30.0",
    "ms": "^2.1.2",
    "normalize-path": "^3.0.0",
    "npm-packlist": "^2.1.2",
    "p-limit": "^3.0.2",
    "parse-glob": "^3.0.4",
    "parse-json": "^5.1.0",
    "quick-lru": "^5.1.1",
    "resolve-from": "^5.0.0",
    "rollup": "^2.79.1",
    "semver": "^7.3.4",
    "terser": "^5.16.8",
    "v8-compile-cache": "^2.1.1",
    "zod": "^3.21.4"
  },
  "preconstruct": {
    "entrypoints": [
      "./cli.ts",
      "./worker.ts"
    ]
  },
  "devDependencies": {
    "@types/estree": "*",
    "escape-string-regexp": "^4.0.0",
    "spawndamnit": "^2.0.0",
    "strip-ansi": "^6.0.0"
  }
}
